#include <iostream>
using namespace std;

int main() {
  double fahrenheit, celsius;

  // Solicita o valor em graus Fahrenheit
  cout << "Digite o valor em graus Fahrenheit: ";
  cin >> fahrenheit;

  // Converte Fahrenheit para Celsius
  celsius = (fahrenheit - 32.0) * (5.0/9.0);

  // Imprime o resultado em graus Celsius
  cout << "O valor em graus Celsius e: " << celsius << endl;

  return 0;
}

